{
    "version": 2,
    "framework": null,
    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.7.0"
      }
    },
    "routes": [
      {
        "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))$",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production"
    },
    "outputDirectory": "."
  }